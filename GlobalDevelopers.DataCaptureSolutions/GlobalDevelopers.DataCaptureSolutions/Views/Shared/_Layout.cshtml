<!DOCTYPE html>
<html  lang="en">
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - Data Capture Solutions</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/thirdpartycss")
    @Scripts.Render("~/bundles/modernizr")

    
</head>
<body>
    
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                    <img height="50" width="150" src="~/Content/Images/data-capture-solutions.png" alt="Data Capture Solutions Ltd">
                </a>
                
                @*<img alt="Data Capture Solutions Ltd" src="~/Content/Images/data-capture-solutions.png" />*@
                @*@Html.ActionLink("Data Capture Solutions", "Index", "Home", new { area = "" }, new { @class = "navbar-brand hidden-sm hidden-md hidden-lg" })*@
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> Trivender Singh <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="#"><i class="fa fa-fw fa-user"></i> Profile</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-envelope"></i> Inbox</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-gear"></i> Settings</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-power-off"></i> Log Out</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li>
                        <a href="#"><i class="fa fa-fw fa-dashboard"></i> Folders</a>
                        <ul>
                            <li><a>Invoice-1</a></li>
                            <li><a>Payment-1</a></li>
                            <li><a>Booking-1</a>
                                <ul>
                                    <li><a>First</a></li>
                                    <li><a>Second</a></li>
                                    <li><a>Third</a></li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript:;" data-toggle="collapse" data-target="#demo"><i class="fa fa-fw fa-arrows-v"></i> Saved Searches <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="demo" class="collapse">
                            <li>
                                <a href="">All Invoices</a>
                            </li>
                            <li>
                                <a href="">Highest Price</a>
                            </li>
                        </ul>
                    </li>
                    @*<li>
                        @Html.ActionLink("Alternate Table Style", "AlternateStyle", "Home")
                    </li>*@
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>
        <div id="page-wrapper">
            <div class="container-fluid">
                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        
                        @RenderBody()
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-dashboard"></i> @Html.ActionLink("Data Capture Solutions", "Index", "Home", new {area = ""})
                            </li>
                            <li class="active">
                                <i class="fa fa-file"></i> Home Page
                            </li>
                        </ol>
                        <hr/>
                        <footer>
                            <p>&copy; @DateTime.Now.Year - Global Developers Limited</p>
                        </footer>
                    </div>
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/thirdpartyscripts")
    @RenderSection("scripts", required: false)

    
    
</body>
</html>
